{
    "extends": [
        "airbnb",
        "prettier",
        "prettier/react"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2016,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "parser": "babel-eslint",
    "env": {
        "es6":true,
        "browser": true,
        "node": true
    },
    "rules": {
        "react/destructuring-assignment": [true, { "extensions": [".jsx"] }],
        "react/prop-types": 0,
        "react/jsx-filename-extension": 0,
        "spaced-comment": 0,
        "func-names": 0,
        "jsx-a11y/anchor-is-valid": 0
    }
}